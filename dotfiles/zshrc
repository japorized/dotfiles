  source $HOME/.aliases
  source $HOME/.functions
  export EDITOR=$(which nvim)
  {%@@ if profile == "Earl-Grey.local" @@%}
  source $HOME/.exports
  {%@@ endif @@%}
  export LANG=en_US.UTF-8
